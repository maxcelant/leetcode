views:
  - type: table
    name: Table
    filters:
      and:
        - file.hasTag("nvidia")
    order:
      - file.name
      - last_attempt
      - rate
    sort:
      - property: rate
        direction: ASC
      - property: last_attempt
        direction: ASC
    cardSize: 150
    columnSize:
      file.name: 466
      note.last_attempt: 271
