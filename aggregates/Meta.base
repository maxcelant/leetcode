views:
  - type: table
    name: Table
    filters:
      and:
        - file.hasTag("meta")
    order:
      - file.name
      - file.tags
      - last_attempt
      - rate
    sort:
      - property: rate
        direction: ASC
      - property: file.name
        direction: DESC
    columnSize:
      file.name: 461
