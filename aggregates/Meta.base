views:
  - type: table
    name: Table
    filters:
      and:
        - file.hasTag("meta")
    order:
      - file.name
      - last_attempt
      - rate
    sort:
      - property: rate
        direction: ASC
      - property: last_attempt
        direction: ASC
    columnSize:
      file.name: 524
      note.rate: 140
